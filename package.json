{
  "name": "datzit",
  "version": "0.0.1",
  "description": "Visualise git log in new and exciting ways",
  "private": false,
  "keywords": [
    "git",
    "git log",
    "git dashboard",
    "git stats",
    "git visualization",
    "dashboard",
    "visualization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamyguy/datzit"
  },
